                        </exclude>
                    </excludes>
                </configuration>
            </plugin>
            <plugin>
                <groupId>cn.hellobike.tools</groupId>
                <artifactId>mybatis-helper</artifactId>
                <version>1.3-SNAPSHOT</version>
                <configuration>
                    <url>jdbc:mysql://localhost:3306/web_api?useSSL=false&amp;serverTimezone=UTC&amp;allowPublicKeyRetrieval=true</url>
                    <username>root</username>
                    <password>12345678</password>
                </configuration>
            </plugin>
        </plugins>
    </build>

